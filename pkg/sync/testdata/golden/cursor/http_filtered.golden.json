{
  "mcpServers": {
    "stdio-server": {
      "command": "npx",
      "args": ["-y", "@example/mcp-stdio"],
      "_managedBy": "agentctl"
    },
    "stdio-with-env": {
      "command": "node",
      "args": ["server.js", "--port", "3000"],
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "mcp:*"
      },
      "_managedBy": "agentctl"
    }
  }
}
